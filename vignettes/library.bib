issn = {17483190},
journal = {Bioinspiration and Biomimetics},
keywords = {3-DOF servosphere,flight muscle electromyograms,silkworm moth},
month = {dec},
number = {1},
pages = {016005},
pmid = {27922836},
publisher = {IOP Publishing},
title = {{A novel method for full locomotion compensation of an untethered walking insect}},
url = {http://stacks.iop.org/1748-3190/12/i=1/a=016005?key=crossref.26868356e5c79d7bfea3e37a2e2f8533},
