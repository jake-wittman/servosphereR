month = {jun},
number = {3},
pages = {636--649},
pmid = {18559169},
publisher = {Entomological Society of America},
title = {{World distribution of female flight and genetic variation in {\textless}i{\textgreater}Lymantria dispar{\textless}/i{\textgreater} (Lepidoptera: Lymantriidae).}},
