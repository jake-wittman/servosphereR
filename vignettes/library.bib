issn = {13652311},
journal = {Ecological Entomology},
keywords = {Arctiidae,Foraging,caterpillars,locomotion,orientation,polarized light,polyphagy,woolly bears},
month = {nov},
number = {4},
pages = {375--386},
publisher = {Blackwell Publishing Ltd},
title = {{Patterns of locomotion of polyphagous arctiid caterpillars in relation to foraging}},
url = {http://doi.wiley.com/10.1111/j.1365-2311.1989.tb00939.x},
