file = {:C$\backslash$:/Users/wittm094/Documents/Mendeley Desktop/Palareti et al. - 2016 - Comparison between different D-Dimer cutoff values to assess the individual risk of recurrent venous thromboemb.pdf:pdf},
isbn = {4955139574},
