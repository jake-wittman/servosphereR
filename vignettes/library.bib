eprint = {0901.4011},
file = {:C$\backslash$:/Users/wittm094/Documents/Mendeley Desktop/Gelman et al. - 2008 - A weakly informative default prior distribution for logistic and other regression models.pdf:pdf},
isbn = {19326157},
