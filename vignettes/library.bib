file = {:C$\backslash$:/Users/wittm094/Documents/Mendeley Desktop/Liebhold et al. - 1994 - Landscape characterization of forest susceptibility to gypsy moth defoliation.pdf:pdf},
isbn = {0015-749X},
journal = {Forest Science},
keywords = {Geographical Information System,Lepidoptera,Lymantria dispar,Lymantriidae},
number = {1},
pages = {18--29},
publisher = {Society of American Foresters},
title = {{Landscape characterization of forest susceptibility to gypsy moth defoliation}},
url = {http://www.ingentaconnect.com/content/saf/fs/1994/00000040/00000001/art00005},
volume = {40},
year = {1994}
