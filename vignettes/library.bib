journal = {The Journal of experimental biology},
keywords = {blue and probably in,butterfly,colour vision,composition of the stimuli,depends on the spectral,discrimination does,in the,lepidoptera,papilio spp,polarisation vision,the green range,vision},
number = {14},
pages = {2469--2480},
pmid = {11511662},
title = {{Polarisation-dependent colour vision in Papilio butterflies.}},
url = {http://jeb.biologists.org/content/jexbio/204/14/2469.full.pdf},
volume = {204},
year = {2001}
}
@article{Murtaugh2007,
