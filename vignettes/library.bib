doi = {10.1016/j.foreco.2007.12.008},
file = {:C$\backslash$:/Users/wittm094/Documents/Mendeley Desktop/Roden, Mattson - 2008 - Rapid induced resistance and host species effects on gypsy moth, iLymantria dispari (L.) Implications for outbre.pdf:pdf},
isbn = {0378-1127},
issn = {03781127},
journal = {Forest Ecology and Management},
keywords = {Defoliation,Gypsy moth,Induced plant defenses,Larval fitness,Rapid induced resistance},
number = {5-6},
pages = {1868--1873},
title = {{Rapid induced resistance and host species effects on gypsy moth, {\textless}i{\textgreater}Lymantria dispar{\textless}/i{\textgreater} (L.): Implications for outbreaks on three tree species in the boreal forest}},
volume = {255},
