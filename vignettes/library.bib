journal = {Ecography},
number = {7},
pages = {648--658},
title = {{Landscape diversity slows the spread of an invasive forest pest species}},
url = {https://s3.amazonaws.com/academia.edu.documents/42475734/Landscape{\_}diversity{\_}slows{\_}the{\_}spread{\_}of{\_}20160209-25129-19yiuxm.pdf?AWSAccessKeyId=AKIAIWOWYYGZ2Y53UL3A{\&}Expires=1507827939{\&}Signature=unaVXZ5ysxf6{\%}2F24s0AiHwlm9cgE{\%}3D{\&}response-content-disposition=inli},
