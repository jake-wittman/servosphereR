doi = {10.1016/j.biocontrol.2016.01.011},
file = {:C$\backslash$:/Users/wittm094/Documents/Mendeley Desktop/Watt et al. - 2016 - Reproductive and developmental biology of the emerald ash borer parasitoid Spathius galinae (Hymenoptera Braconidae.pdf:pdf},
isbn = {1049-9644},
issn = {10499644},
journal = {Biological Control},
keywords = {Agrilus planipennis,Biological control,Fecundity,Life tables,Mass-rearing,Survival},
month = {may},
pages = {1--7},
title = {{Reproductive and developmental biology of the emerald ash borer parasitoid Spathius galinae (Hymenoptera: Braconidae) as affected by temperature}},
