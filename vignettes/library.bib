isbn = {0066-4170},
issn = {0066-4170},
journal = {Annual Review of Entomology},
number = {1},
pages = {447--467},
primaryClass = {0066-4170},
title = {{Searching behavior patterns in insects}},
url = {https://www.annualreviews.org/doi/pdf/10.1146/annurev.en.35.010190.002311 http://www.annualreviews.org/doi/10.1146/annurev.en.35.010190.002311},
volume = {35},
