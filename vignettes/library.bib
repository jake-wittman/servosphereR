journal = {Ecological Modelling},
number = {3-4},
pages = {253--268},
title = {{Simulations on the role of the egg parasite, Ooencyrtus kuvanae (Howard), in the population dynamics of the gypsy moth}},
url = {http://ac.els-cdn.com/0304380083900170/1-s2.0-0304380083900170-main.pdf?{\_}tid=34f4d2d8-926e-11e7-aa65-00000aab0f02{\&}acdnat=1504638949{\_}ad46612877c7d03b1b0c50e257b2f644},
