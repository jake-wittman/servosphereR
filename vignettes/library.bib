journal = {Avian Conservation and Ecology},
keywords = {breakpoint-based threshold,classification and regression tree,classification threshold,ecological,ecological thresholds,logistic,occupancy threshold,piecewise regression,regression,roc analysis,thresholds},
number = {1},
