keywords = {artificial diets,carbonyl reductase,caterpillars,detoxication,esterase,fall armyworm,feeding trials,food plants,glutathione transferase,growth,gypsy moth,host plants,induction,lepidoptera,lymantria-dispar,nitrogen,nutrient deficiencies,nutritional indexes,polysubstrate monooxygenase,water-content},
