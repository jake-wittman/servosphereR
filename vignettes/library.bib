@article{Hanson2013,
abstract = {Emerald ash borer, Agrilus planipennis Fairmaire (Coleoptera: Buprestidae), is an invasive insect that has caused significant ash ( Fraxinus spp.) mortality in North America. Three Chinese parasitoids have been approved for release as part of a classical biological control program for A. planipennis in the United States: Spathius agrili Yang (Hymenoptera: Braconidae), Tetrastichus planipennisi Yang (Hymenoptera: Eulophidae), and Oobius agrili Zhang and Huang (Hymenoptera: Encyrtidae). This study was designed to measure the cold tolerance of the overwintering stage for each parasitoid species in the laboratory. We exposed cold-acclimated and non-cold-acclimated individuals to temperatures from 0 to -35. ??C to determine temperatures that cause body fluids to freeze, mortality after brief exposure, and mortality after long-term exposure. Cold acclimation lowered the supercooling points of S. agrili (median -28.8. ??C) and T. planipennisi (median -29.4. ??C). Median supercooling point for Oobius agrili was -30.5. ??C. Cold acclimation also increased survival of diapausing S. agrili (50{\%} mortality at -27.3 versus -23.7. ??C for non-diapausing S. agrili) during brief cold exposure. T. planipennisi and S. agrili mortality increased over long term cold exposure when held at constant temperature. Half of T. planipennisi are predicted to fail to eclose after exposure to 0, -5, -10, and -15. ??C after *84, 82, 59, and 36. days, respectively, while 50{\%} of S. agrili with diapause induced in one generation would be discolored from cold injury *84. days for all exposure temperatures. Our models characterizing parasitoid mortality due to cold exposure can be used to assess the climatic suitability of a location prior to release. ?? 2013 .},
annote = {-Oobius seems like it might be the most cold hardy, but they only found SCP and not lower lethal temps or lower lethal times so it's hard to know.
-Tetrastichus seems the least cold hardy and is probably chill intolerant, although they suspected there was issues in handling the larvae that resulted in higher mortality
-Chill acclimation improved SCP for Spathius agrili, but not really in Tet
-If the lower lethal temp 50 is above the SCP CI, then the insect is likely chill intolerant.},
author = {Hanson, Anthony A. and Venette, Robert C. and Lelito, Jonathan P.},
doi = {10.1016/j.biocontrol.2013.08.015},
file = {:C$\backslash$:/Users/wittm094/Documents/Mendeley Desktop/Hanson, Venette, Lelito - 2013 - Cold tolerance of Chinese emerald ash borer parasitoids Spathius agrili Yang (Hymenoptera Braconidae),.pdf:pdf},
issn = {10499644},
journal = {Biological Control},
keywords = {Agrilus planipennis,Lower lethal temperature,Lower lethal time,Supercooling point},
month = {dec},
number = {3},
pages = {516--529},
publisher = {Academic Press},
title = {{Cold tolerance of Chinese emerald ash borer parasitoids: Spathius agrili Yang (Hymenoptera: Braconidae), Tetrastichus planipennisi Yang (Hymenoptera: Eulophidae), and Oobius agrili Zhang and Huang (Hymenoptera: Encyrtidae)}},
url = {http://www.sciencedirect.com/science/article/pii/S1049964413001965},
volume = {67},
year = {2013}
}
@article{Brown1983,
abstract = {Computer simulations were run to examine the effects of Ooencrytus kuvanae (Howard) parasitism on gypsy moth populations. Two difference equation models were used, one, a simple theoretical model containing only a few important components, the other, a more comprehensive model with component functions based on field data. The simulations of the first model showed that O. Kuvanae cannot control gypsy moth populations by itself. Egg parasitism was most important during the outbreak and collapse phase of the gypsy moth population cycle, and was of little importance in the endemic phase. The endemic phase was, however, lower and lasted longer with O. kuvanae parasitism than without. Simulations with the second model generally supported these conclusions. The second set of simulations also demonstrated that due to the non-linearity of the density dependence of O. kuvanae parasitism, they gypsy moth populations could be forced into a chaotic regime (unstable, aperiodic population behavior). The variation in parasitism rate from the simulations was compared with that of field populations and found to be essentially the same, indicating that the simulation models are realistic. {\textcopyright} 1983.},
author = {Brown, M W and Williams, F M and {Alan Cameron}, E.},
doi = {10.1016/0304-3800(83)90017-0},
file = {:C$\backslash$:/Users/wittm094/Documents/Mendeley Desktop/Brown, Williams, Alan Cameron - 1983 - Simulations on the role of the egg parasite, Ooencyrtus kuvanae (Howard), in the population dynam.pdf:pdf},
