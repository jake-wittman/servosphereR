issn = {1938-2936},
journal = {Environmental Entomology},
keywords = {a chal-,as dispersal may depend,dispersal,elucidating processes that contribute,hylobius warreni,lenge,mark-recapture,mark-release,matrix habitat,of insect herbivores is,on multiple and inter-,terns of the dispersal,to spatial pat-},
