keywords = {Larus glaucescens,Mytilus californianus,Nucella,Pollicipes polymerus,Sem-ibalanus cariosus,bird predation,community interaction webs,indirect mutualism,interaction strength,intertidal communities,space competition},
number = {751},
pages = {151--165},
title = {{Predicting Direct and Indirect Effects: An Integrated Approach Using Experiments and AN INTEGRATED APPROACH USING EXPERIMENTS AND PATH ANALYSIS'}},
