journal = {Ecological Modelling},
keywords = {Agent-based modeling,Biological control,Complex systems,Emerald ash borer,Geospatial simulation,Tetrastichus planipennisi},
month = {oct},
pages = {310--329},
publisher = {Elsevier},
title = {{Geospatial pest-parasitoid agent based model for optimizing biological control of forest insect infestation}},
