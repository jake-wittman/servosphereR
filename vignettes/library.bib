pages = {55--64},
publisher = {Wiley/Blackwell (10.1111)},
title = {{Variation in temperature and dietary nitrogen affect performance of the gypsy moth ({\textless}i{\textgreater}Lymantria dispar{\textless}/i{\textgreater} L.)}},
