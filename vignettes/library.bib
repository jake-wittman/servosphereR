issn = {19382936},
journal = {Environmental Entomology},
keywords = {Asian gypsy moth,Degree-day,Development time,Temperature},
month = {jul},
number = {4},
pages = {1012--1023},
title = {{Effects of temperature on development of lymantria dispar asiatica and lymantria dispar Japonica (lepidoptera: Erebidae)}},
