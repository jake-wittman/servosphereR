issn = {0270-6474},
journal = {J Neurosci},
keywords = {Animals,Central Nervous System/ physiology,Extremities/innervation/ physiology,Feedback/ physiology,Female,Insects/ physiology,Motor Neurons/physiology,Movement/ physiology,Muscarinic Agonists/pharmacology,Nerve Net/physiology,Pilocarpine/pharmacology,Walking/ physiology},
