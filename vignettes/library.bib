isbn = {1365-2311},
issn = {03076946},
journal = {Ecological Entomology},
keywords = {Biological control,Encapsulation,Immunity,Katydid,Lysozyme,Migration,Nutrition,Phenoloxidase},
month = {oct},
number = {5},
pages = {663--668},
publisher = {Wiley/Blackwell (10.1111)},
title = {{Immediate protein dietary effects on movement and the generalised immunocompetence of migrating Mormon crickets {\textless}i{\textgreater}Anabrus simplex{\textless}/i{\textgreater} (Orthoptera: Tettigoniidae)}},
