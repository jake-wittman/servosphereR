isbn = {19355297},
issn = {02785226},
journal = {Arboriculture and Urban Forestry},
keywords = {Blue ash,Emerald ash borer,Fraxinus,Green ash,Host preference,Insect survey,Invasive pest,White ash,Woodpecker},
number = {5},
pages = {338--349},
title = {{Emerald ash borer (Agrilus planipennis) density and canopy dieback in three North American ash species}},
volume = {33},
year = {2007}
