issn = {00218790},
journal = {Journal of Animal Ecology},
keywords = {Expanding populations,Isolated colonies,Lymantria dispar,Protandry,Stochastic diffusion model,allee effect,establishment,gypsy moth,lymantria dispar,mating,spatial,temporal},
mendeley-tags = {allee effect,establishment,gypsy moth,lymantria dispar,mating,spatial,temporal},
