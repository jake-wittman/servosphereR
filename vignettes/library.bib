title = {{Seeing the forest through the trees: differential dispersal of {\textless}i{\textgreater}Hylobius warreni{\textless}/i{\textgreater} within modified forest habitats.}},
url = {http://ee.oxfordjournals.org/lookup/doi/10.1603/EN08269},
volume = {39},
year = {2010}
}
@article{ONeal2004,
abstract = {ABSTRACT: Harmonic radar technology can be used to track the$\backslash$ndispersal of tagged insects. The tag consists of a wire antenna attached$\backslash$nto a Schottky diode, which uses the original radar signal as$\backslash$nan energy source, re-emitting a harmonic of the transmitted wavelength.$\backslash$nTwo forms of harmonic radar use this basic technology to$\backslash$nstudy insect movement. The more sophisticated form consists of a$\backslash$nground-based scanning radar station that tracks the movement of a$\backslash$ntagged insect on a circular radar display. A simpler, “off-the-shelf”$\backslash$nform of harmonic radar is a commercially available, light-weight,$\backslash$nhandheld transmitter/receiver from RECCO Rescue Systems. We$\backslash$nbriefly review both of these forms and describe our experience$\backslash$nmonitoring the movement of carabid beetles in agricultural habitats$\backslash$nwith the handheld transmitter/receiver. We identified a commercial$\backslash$nsource of diodes compatible with the RECCO transmitter/receiver$\backslash$nand tested several diode and wire combinations. We found that a tag$\backslash$nbuilt with a diode attached to a single section of 8-cm wire (monopole)$\backslash$nwas more appropriate for marking carabids. Tags built from$\backslash$nflexible Teflon-coated wires were an improvement on tags built with$\backslash$nstiff, aluminum wire, but beetle movement was still hindered. In corn$\backslash$nand soybean fields, large carabids (Scarites quadriceps Chaudoir and$\backslash$nHarpalus pennsylvanicus, (DeG.) Coleoptera: Carabidae) could be$\backslash$nrecaptured even when they burrowed out of sight 3 to 9 cm below$\backslash$nthe soil surface. We discuss the trade-offs between tag detection and$\backslash$ndurability that occur when designing a tag for a given organism.$\backslash$nAlthough the technique shows promise, producing a tag that does$\backslash$nnot hinder movement of the target insect in the field will require$\backslash$nfurther development.},
author = {O'Neal, M.E and Landis, Doug A. and Rothwell, E. and Kempel, L. and Reinhard, D.},
doi = {10.1146/annurev-ento-120709-144820},
