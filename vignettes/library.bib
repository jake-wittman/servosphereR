file = {:C$\backslash$:/Users/wittm094/Documents/Mendeley Desktop/Wittman et al. - 2013 - Evaluation of land use and water quality in an agricultural watershed in the USA indicates multiple sources of b.pdf:pdf},
issn = {01676369},
journal = {Environmental Monitoring and Assessment},
keywords = {Benthic macroinvertebrates,E. coli,Livestock,Nitrogen,Phosphorus,Water quality},
month = {dec},
number = {12},
pages = {10395--10420},
pmid = {23873513},
publisher = {Springer Netherlands},
title = {{Evaluation of land use and water quality in an agricultural watershed in the USA indicates multiple sources of bacterial impairment}},
